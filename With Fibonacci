#include<stdio.h> 
 int main() 
 { 
     int a=0,j=1,i,n; 
     printf("Enter the number : "); 
     scanf("%d",&n); 
     printf("Fibonacci : "); 
     for(int i=0;i<=n;i++) 
     { 
         printf("%d ",fib(i)); 
     } 
 } 
     int fib(int n) 
 { 
         if(n<=1) 
         { 
              return n; 
         } 
         else 
         { 
              return fib(n-1)+fib(n-2); 
         } 
 
