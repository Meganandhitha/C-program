#include<stdio.h> 
 void main() 
 { 
    char hey; 
    scanf("%c",&hey); 
    switch(hey) 
    { 
        case 'a': 
        printf("IT IS AN VOWEL"); 
        break; 
        case 'e': 
        printf("IT IS AN VOWEL"); 
        break; 
        case 'i': 
        printf("IT IS AN VOWEL"); 
        break; 
        case 'o': 
        printf("IT IS AN VOWEL"); 
        break; 
        case 'u': 
        printf("IT IS AN VOWEL"); 
        break; 
        default: 
        printf("IT IS NOT AN VOWEL"); 
    } 
  
 
