#include<stdio.h> 
 void main() 
 { 
         int a,b,c; 
         scanf("%d %d %d",&a,&b,&c); 
         if(a>b&&a>c) 
         { 
             printf("A is the Biggest Number"); 
         } 
         else if(a<b&&b>c) 
         { 
             printf("B is the Biggest Number"); 
         } 
         else 
         { 
             printf("C is the Biggest Number"); 
         } 
  
 
